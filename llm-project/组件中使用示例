import { streamChatCompletion, switchModel } from '@/services/aiService';

// 切换模型
switchModel('gpt-4');

// 发起请求
const abort = await streamChatCompletion(
    {
        prompt: '你好',
        max_tokens: 1000,
        temperature: 0.7
    },
    {
        onStart: () => console.log('开始流式传输'),
        onData: (chunk) => console.log('收到数据块:', chunk),
        onComplete: () => console.log('传输完成'),
        onError: (err) => console.error('发生错误:', err)
    }
);

// 取消请求
// abort();